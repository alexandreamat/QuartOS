{
  "compilerOptions": {
    "target": "es2017",
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "./src",
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/features/dashboard/**/*",
    "src/features/transactionold/**/*",
    "src/features/accountold/storage.tsx",
    "src/features/settings/storage.tsx",
  ]
}